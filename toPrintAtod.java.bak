import java.util.Scanner;
class toPrintAtod 
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("enter the rows");
		int rows = sc.nextInt();
		System.out.println("enter the columns");
		int col = sc.nextInt();
		char ch ='a';
			for(int i=1; i<=rows; i++)
			{
				for(int j=1; j<=col; j++)
				{
					System.out.print(ch+ " ");
				}
			ch++;
			System.out.println();
		}
	}
}
